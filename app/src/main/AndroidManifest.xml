<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="zone.com.zadapter3">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="zone.com.zadapter3kt.MainKTActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="zone.com.zadapter3kt.RecyclerKTActivity"></activity>
        <activity android:name="zone.com.zadapter3kt.NineRecyclerKTActivity"></activity>
        <activity android:name="zone.com.zadapter3kt.ZRefreshKTActivity"></activity>
        <activity android:name="zone.com.zadapter3kt.FullyRecyclerKTActivity"></activity>
        <activity android:name="zone.com.zadapter3kt.StickyKTActivity"></activity>
        <activity android:name="zone.com.zadapter3kt.HexoLayoutKTActivity"></activity>
        <activity android:name="zone.com.zadapter3kt.LayoutKTActivity"></activity>
        <activity android:name=".StickyActivity"></activity>
        <activity android:name="zone.com.zadapter3kt.RecyclerActivityAnimator"></activity>
        <activity android:name="zone.com.zadapter3kt.RecyclerActivityScroll"></activity>
    </application>

</manifest>